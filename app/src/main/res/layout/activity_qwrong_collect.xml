<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sunlands.intl.yingshi.ui.question.QWrongCollectActivity">
    <com.sunlands.comm_core.weight.commtitle.CommTitle
        android:id="@+id/common_title"
        android:layout_width="match_parent"
        app:lineVisible="false"
        android:layout_height="44dp"/>
    <net.lucode.hackware.magicindicator.MagicIndicator
        android:id="@+id/mi_q_wrong_collect"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_gravity="center_horizontal"
        android:layout_height="44dp" />
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_q_wrong_collect"
        app:layout_constraintTop_toBottomOf="@id/mi_q_wrong_collect"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
</android.support.constraint.ConstraintLayout>
